@model IEnumerable<XMLData.Models.Photos>

@{
    ViewBag.Title = "PhotoIndex";
}


@{
    var grid = new WebGrid(Model, canPage: false,
    selectionFieldName: "selectedRow");
}
<div class="row">
    <div id="gridContent">
        @grid.GetHtml(tableStyle: "webGrid",
            headerStyle: "header",
            alternatingRowStyle: "alt",
            selectedRowStyle: "select",
            columns: grid.Columns(
            grid.Column("PhotoId", "PhotoId"),
            grid.Column("TFile", "TFile"),
            grid.Column("File", "File"),
            grid.Column("TCaption", "TCaption")
     ))
    </div>
</div>
 